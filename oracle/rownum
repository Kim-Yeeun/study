
--rownum이 1부터 3 사이인 게시글 출력하기
SELECT * 
FROM NOTICE 
WHERE ROWNUM BETWEEN 1 AND 3;

--rownum이 3부터 5 사이인 게시글 출력은 위의 쿼리문에서 숫자만 바꾼다고 출력되지 않음.
SELECT * 
FROM 
  (SELECT ROWNUM NUM, NOTICE.* 
  FROM NOTICE) 
WHERE NUM BETWEEN 3 AND 5; 

SELECT *
FROM
    (SELECT
        T1.NAME,
        T1.DATETIME
    FROM ANIMAL_INS T1
        LEFT OUTER JOIN ANIMAL_OUTS T2
        ON T1.ANIMAL_ID = T2.ANIMAL_ID
    WHERE T2.DATETIME IS NULL
    ORDER BY T1.DATETIME)
WHERE ROWNUM <= 3


