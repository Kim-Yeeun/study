
# while문 사용

n1 = 1
n2 = 1

print(n1)
print(n2)
count = 2

while count <= 48:
    n1 = n1 + n2
    print(n1)
    n2 = n2 + n1
    print(n2)
    count += 2


# for문 사용

n1 = 1
n2 = 1

print(n1)
print(n2)

for i in range(0, 24):
    n1 = n1 + n2 # 2 5
    n2 = n1 + n2 # 3 8
    print(n1)
    print(n2)
    
# 재귀 사용

def fib(n):
    if n == 1 or n == 2:
        return 1
    else:
        return fib(n - 1) + fib(n - 2)

for i in range(1, 11):
    print(fib(i))
    
